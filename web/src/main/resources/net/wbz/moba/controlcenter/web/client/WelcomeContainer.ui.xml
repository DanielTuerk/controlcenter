    <b:Container fluid="false">
        <b:Row>
            <b:Column size="SM_5">

                <b:Panel>
                    <b:PanelHeader>
                        <b:Heading size="H3" text="Create Construction"/>
                    </b:PanelHeader>
                    <b:PanelBody>
                        <b:Form type="INLINE">
                            <b:FieldSet>
                                <b:FormGroup>
                                    <b:TextBox ui:field="txtCreateName" placeholder="Enter name"/>
                                </b:FormGroup>
                                <b:FormGroup>
                                    <b:Button ui:field="btnCreateConstruction">Send</b:Button>
                                </b:FormGroup>
                            </b:FieldSet>
                        </b:Form>
                    </b:PanelBody>
                    <b:PanelFooter>
                    </b:PanelFooter>
                </b:Panel>

            </b:Column>
            <b:Column size="SM_5" offset="SM_2">
                <b:Panel>
                    <b:PanelHeader>
                        <b:Heading size="H3" text="Load Construction"/>
                    </b:PanelHeader>
                    <b:PanelBody>
                        <b:LinkedGroup ui:field="listGroupConstructions">
                                </b:LinkedGroup>
                    </b:PanelBody>
                </b:Panel>
            </b:Column>
        </b:Row>
    </b:Container>


