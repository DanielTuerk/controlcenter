<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <b:Form type="HORIZONTAL">
    <b:FieldSet>
      <b:Legend ui:field="legend"/>



      <b:FormGroup>
        <b:FormLabel for="formName" addStyleNames="col-lg-2">Name</b:FormLabel>
        <g:FlowPanel addStyleNames="col-lg-10">
          <b:TextBox ui:field="txtName"/>
        </g:FlowPanel>
      </b:FormGroup>


      <b:FormGroup>
        <b:FormLabel for="formAddress" addStyleNames="col-lg-2">Address</b:FormLabel>
        <g:FlowPanel addStyleNames="col-lg-10">
          <b:TextBox ui:field="txtAddress"/>
        </g:FlowPanel>
      </b:FormGroup>


      <b:FormGroup>
        <b:FormLabel for="formBit" addStyleNames="col-lg-2">Bit</b:FormLabel>
        <g:FlowPanel addStyleNames="col-lg-10">
          <b:TextBox ui:field="txtBit"/>
        </g:FlowPanel>
      </b:FormGroup>
</b:FieldSet>
    <b:FieldSet>
      <b:Legend >Driving level adjustment</b:Legend>

      <b:FormGroup>
        <b:FormLabel for="formCountry" addStyleNames="col-lg-2">Type</b:FormLabel>
        <g:FlowPanel addStyleNames="col-lg-10">
          <b:ListBox ui:field="selectDrivingLevelAdjustType">
          </b:ListBox>
        </g:FlowPanel>
      </b:FormGroup>

      <b:FormGroup>
        <b:FormLabel for="formAddress" addStyleNames="col-lg-2">Forward driving level</b:FormLabel>
        <g:FlowPanel addStyleNames="col-lg-10">
          <b:TextBox ui:field="txtForwardTargetDrivingLevel"/>
        </g:FlowPanel>
      </b:FormGroup>
      <b:FormGroup>
        <b:FormLabel for="formAddress" addStyleNames="col-lg-2">Backward driving level</b:FormLabel>
        <g:FlowPanel addStyleNames="col-lg-10">
          <b:TextBox ui:field="txtBackwardTargetDrivingLevel"/>
        </g:FlowPanel>
      </b:FormGroup>

    </b:FieldSet>
  </b:Form>
</ui:UiBinder>