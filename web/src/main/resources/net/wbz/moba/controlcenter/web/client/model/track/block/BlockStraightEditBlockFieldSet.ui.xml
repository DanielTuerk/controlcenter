  xmlns:cc="urn:import:net.wbz.moba.controlcenter.web.client.components">

  <b:FieldSet>

    <b:FormGroup>
      <b:FormLabel ui:field="lblLeftBlock"/>
      <cc:TrackBlockSelect ui:field="leftBlockSelect"/>
    </b:FormGroup>

    <b:FormGroup>
      <b:FormLabel text="Middle"/>
      <cc:TrackBlockSelect ui:field="middleBlockSelect"/>
    </b:FormGroup>

    <b:FormGroup>
      <b:FormLabel ui:field="lblRightBlock"/>
      <cc:TrackBlockSelect ui:field="rightBlockSelect"/>
    </b:FormGroup>
  </b:FieldSet>

</ui:UiBinder>