<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:HTMLPanel styleName="flexbox-item-grow">
    <b:Container fluid="true">
      <b:Row>
        <b:Column size="SM_12">
          <b:PageHeader text="Scenario"/>
          <b:Container ui:field="scenarioBody"/>
        </b:Column>
      </b:Row>

      <b:Row>
        <b:Column size="SM_12">
          <b:PageHeader text="Route"/>
          <b:Container ui:field="routeBody"/>
        </b:Column>
      </b:Row>

      <b:Row>
        <b:Column size="SM_12">
          <b:PageHeader text="Stations"/>
          <b:Container ui:field="stationBody"/>
        </b:Column>
      </b:Row>

    </b:Container>
  </g:HTMLPanel>
</ui:UiBinder>