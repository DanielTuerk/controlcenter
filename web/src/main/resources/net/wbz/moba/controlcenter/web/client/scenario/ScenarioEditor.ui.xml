<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

  <b:Container fluid="true">

    <b:Row>
      <b:Column size="SM_12">
        <b:Panel>
          <b:PanelHeader>
            <b:Heading size="H4" text="Scenario"/>
          </b:PanelHeader>
          <b:PanelBody ui:field="scenarioEditPanel">
          </b:PanelBody>
        </b:Panel>
      </b:Column>
    </b:Row>

    <b:Row>
      <b:Column size="SM_6">

        <b:Panel>
          <b:PanelHeader>
            <b:Heading size="H4" text="Stations"/>
          </b:PanelHeader>
          <b:PanelBody ui:field="stationsEditPanel">
          </b:PanelBody>
        </b:Panel>

      </b:Column>


      <b:Column size="SM_6">

        <b:Panel>
          <b:PanelHeader>
            <b:Heading size="H4" text="Route"/>
          </b:PanelHeader>
          <b:PanelBody ui:field="routeEditPanel">
          </b:PanelBody>
        </b:Panel>

      </b:Column>

    </b:Row>

  </b:Container>

</ui:UiBinder>