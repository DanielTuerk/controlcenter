<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:b-gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
>

  <ui:style>
    td {
      white-space: pre;
    }
  </ui:style>
  <b:Container fluid="true" ui:field="stationContainer">
    <b:Button type="PRIMARY" icon="PLUS" text="Create Station" ui:field="btnCreateStation"/>
    <b-gwt:CellTable ui:field="stationTable" bordered="true" condensed="true" striped="true" hover="true"
      height="100%" width="100%"/>
  </b:Container>

</ui:UiBinder>