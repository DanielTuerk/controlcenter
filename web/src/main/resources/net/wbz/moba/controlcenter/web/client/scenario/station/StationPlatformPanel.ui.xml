<ui:UiBinder xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:cc="urn:import:net.wbz.moba.controlcenter.web.client.components"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:ui='urn:ui:com.google.gwt.uibinder'>

  <b:Panel>
    <b:PanelBody>
      <b:Form type="HORIZONTAL">
        <b:FieldSet>
          <b:FormGroup>
            <b:FormLabel addStyleNames="col-lg-2" for="formName">Name</b:FormLabel>
            <g:FlowPanel addStyleNames="col-lg-10">
              <b:TextBox ui:field="txtName"/>
            </g:FlowPanel>
          </b:FormGroup>
          <b:FormGroup>
            <b:FormLabel addStyleNames="col-lg-2">Blocks</b:FormLabel>
            <g:FlowPanel addStyleNames="col-lg-10">
              <cc:TrackBlockMultiSelect ui:field="trackBlockSelect"/>
            </g:FlowPanel>
          </b:FormGroup>
          <b:Button icon="TRASH" type="DANGER" ui:field="btnDelete"/>
        </b:FieldSet>
      </b:Form>
    </b:PanelBody>
  </b:Panel>

</ui:UiBinder>