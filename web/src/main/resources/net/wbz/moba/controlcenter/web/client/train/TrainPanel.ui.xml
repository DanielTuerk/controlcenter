<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:b-gwt="urn:import:org.gwtbootstrap3.client.ui.gwt">

  <b:Container fluid="true" width="100%">
    <b:Button type="PRIMARY" icon="PLUS" text="Create Scenario" ui:field="btnCreateTrain"/>
    <b-gwt:CellTable ui:field="trainTable" bordered="true" condensed="true" striped="true" hover="true"
       width="100%"/>
    <b:Pagination ui:field="pagination" paginationSize="SMALL"/>
  </b:Container>

</ui:UiBinder>