<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
>
  <ui:style>
    .addressItem {
      float: left;
    }
  </ui:style>
  <g:FlowPanel addStyleNames="{style.addressItem}">
    <b:Label ui:field="lblAddress"/>
    <b:ButtonGroup dataToggle="BUTTONS" ui:field="btnGroup">
    </b:ButtonGroup>
  </g:FlowPanel>

</ui:UiBinder>