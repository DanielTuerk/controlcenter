>

  <ui:style>
  </ui:style>

  <b:Panel>
    <b:PanelHeader>
      <g:FlowPanel>
        <g:HTMLPanel>
          <b:Label ui:field="lblName"/>
          <g:FlowPanel>
            <b:Label ui:field="lblState"/>
            <b:Label ui:field="lblStateDetails"/>
          </g:FlowPanel>
        </g:HTMLPanel>
        <b:Button ui:field="btnCollapse" icon="CARET_SQUARE_O_DOWN" dataToggle="COLLAPSE"/>
      </g:FlowPanel>

    </b:PanelHeader>
    <b:PanelBody>
      <b:PanelCollapse ui:field="contentCollapse">

      </b:PanelCollapse>
    </b:PanelBody>
  </b:Panel>

</ui:UiBinder>